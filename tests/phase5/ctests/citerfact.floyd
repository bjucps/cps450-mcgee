

class CIterfact is

  num: int
  num2: int
  isOk: boolean

  Fact(num: int): int is
    answer: int
  begin
    answer := 1
    loop while num > 0
      answer := answer * num
      num := num - 1
    end loop
    Fact := answer
    out.writeint(0 + answer - answer)  ~ see if we can mess up return value -- heh heh heh
  end Fact

  start() is
  begin 
    num := in.readint()
    if num > 0 then
      num := Fact(num)
      out.writeint(num)
    end if
  end start
end CIterfact
