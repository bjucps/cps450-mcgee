type Point(x: int, y: int) :=
  member getX(): int :=
    x
  
  member getY(): int :=
    y
  
  member print() :=
    let z := printint(x)
    printint(y)

  member setX(newX: int) :=
    x := newX
  
  member setY(newY: int) :=
    y := newY
end

let doit(p: Point) :=
  p.print() # null pointer exception occurs when called from line 26

