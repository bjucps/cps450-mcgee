# requires EC to be implemented
let start() :=
    let mov := printint(12 * readint())   # Enter 10, prints 120
    let mov1 := printint(mov / ~1)        # prints -120
    let mov2 := printint(mov * ~1)        # prints -120
    let x := 0
    let y := 1
    loop while ! (printint(x := x + 1) > (y := 2) + 1) do   # 1 -1 -2 2 -1 -2 3 -1 -2, incrementing x by 1, resetting y to 2 at start of loop!
      loop while y > ~2 do
        if ! (y >= 0) then
          printint(y := y - 1)
        else
          y := y - 1
        end
      end
    end